<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/listFragemnt">

    <fragment
        android:id="@+id/listFragemnt"
        android:name="com.fraime.android.rm2.presentation.ui.list.ListFragemnt"
        android:label="ListFragemnt" >
        <action
            android:id="@+id/action_listFragemnt_to_detailsFragment"
            app:destination="@id/detailsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="com.fraime.android.rm2.presentation.ui.details.DetailsFragment"
        android:label="DetailsFragment" />
</navigation>